<% include template/header.ejs %>
<body id="page-top" ng-app="App" ng-controller="indexController">
<div class="container" style="margin-top : 100px">
    <div class="col-md-12">
        <div class="form-group col-md-3">
            <label for="email">LocationName:</label>
            <input type="text" ng-model="locationName" class="form-control" id="email">
         </div>
         <div class="form-group col-md-3">
            <label for="email">Description:</label>
            <input type="text" ng-model="description" class="form-control" id="email">
          </div>
          <div class="form-group col-md-3">
            <label for="email">latitude:</label>
            <input type="text" ng-model="lat" class="form-control" id="email">
          </div>
          <div class="form-group col-md-3">
            <label for="email">longitude:</label>
            <input type="text" ng-model="lot" class="form-control" id="email">
          </div>
          
    </div>
    <div class="col-md-12">
        <button type="submit" class="btn btn-default" ng-click="store()">Submit</button>
        
    </div>
    <div class="col-md-12">
      <table>
            <thead>
              <th>locationName</th>
              <th>DescriptionName</th>
              <th>latitude</th>
              <th>longitude</th>
            </thead>
            <tbody class="places">
              <% datas.forEach(function(data){ %> 
                <tr>
                  <td><%= data.locationName%></td>
                  <td><%= data.description%></td>
                  <td><%= data.latitude%></td>
                  <td><%= data.longitude%></td>
                </tr>
              <% });%>
            </tbody>
          </table>  
    </div>    
</div>




<script src="js/jquery.min.js"></script>
<script src="js/angular.min.js"></script>
<script src="js/angular-animate.min.js"></script>
<script src="js/main.js"></script>
</body>
</html>
